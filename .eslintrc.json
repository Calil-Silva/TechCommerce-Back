{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            { "js": "ignorePackages"}
        ],
        "comma-dangle": 0,
        "no-undef": 0,
        "radix": 0,
        "operator-linebreak": 0,
        "import/prefer-default-export": 0
    }
}
